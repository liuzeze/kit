<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lz.com.kit">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PermissionActivity"></activity>
        <activity android:name=".MultiCheckedActivity" />
        <activity android:name=".MultiUnCancelCheckedActivity" />
        <activity android:name=".SingleUnCancelCheckedActivity" />
        <activity android:name=".PageRecyclerViewActivity" />
        <activity android:name=".SingleSelectActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ScrollViewActivity" />
        <activity android:name=".RecyclerViewActivity" />
        <activity android:name=".IntervalActivity" />
        <activity android:name=".DialogActivity" />
        <activity android:name=".IocActivity" />
        <activity android:name=".ShopActivity" />
        <activity android:name=".CitySelectActivity" />
        <activity android:name=".TitleActivity" />
    </application>

</manifest>